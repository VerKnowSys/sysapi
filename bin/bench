#!/bin/sh
# blame: @dmilith
# 2018-11-22-0129-1542846591


. bin/build


_cycles=10

printf "\n\nBench: SysAPI: Running measurement ${_cycles} times (sequentially one by one)\n\n"
for i in $(seq 0 ${_cycles}); do
    RUSTFLAGS="-lkvm -lprocstat" cargo run --bin kernelbench ${_cargo_release_flag}
done
